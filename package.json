{
  "author": "Justin Domingue",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@uniswap/token-lists": "^1.0.0-beta.27",
    "dts-cli": "^1.4.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
